{
  "private": true,
  "name": "@winfe/winex",
  "version": "0.0.1",
  "description": "Create a Nuxt.js App in seconds.",
  "files": [
    "lib"
  ],
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "bootstrap": "rimraf -f ./packages/.DS* &&  rimraf -rf ./packages/**/node_modules && lerna bootstrap",
    "build": "father-build",
    "changelog": "lerna-changelog",
    "clean": "lerna clean -y",
    "debug": "umi-test",
    "publish": "node ./scripts/release.js",
    "version": "conventional-changelog -p angular -i CHANGELOG.md -s  && git add CHANGELOG.md",
    "docs:dev": "dumi dev",
    "docs:build": "dumi build",
    "test:cicd": "node ./scripts/test.js",
    "codereview": "node ./scripts/codereview.js",
    "test": "jest"
  },
  "dependencies": {
    "cross-spawn": "^7.0.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^26.0.23",
    "chalk": "^4.1.0",
    "conventional-changelog-cli": "^2.0.31",
    "dumi": "^1.0.34",
    "eslint": "^6.8.0",
    "father-build": "^1.18.6",
    "husky": "^4.2.5",
    "jest": "^27.0.3",
    "lerna": "^3.22.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.2"
  },
  "license": "MIT",
  "publishConfig": {
    "registry": "http://registry.npmjs.org/"
  }
}
